@using InsuranceExercise.ViewModels
@model List<InsuranceBuyersVm>

<div class="row">
    <div class="card">
        <div class="card-header text-center">
            <h3 class="m-0">All Insurance Estimates</h3>
        </div>
        <div class="card-body">
            @foreach (var allEstimates in Model)
            {
            <div class="container">
                <div id="inquiryRow" class="row">
                    <div class="col-sm">
                        @allEstimates.FirstName
                    </div>
                    <div class="col-sm">
                        @allEstimates.LastName
                    </div>
                    <div class="col-sm">
                        @allEstimates.EmailAddress
                    </div>
                </div>
            </div>
            }
        </div>
    </div>
</div>

